{
    "name": "@onivoro/server-common",
    "version": "0.1.3",
    "repository": {
        "url": "git+https://github.com/onivoro/server-common.git"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/*"
    ],
    "scripts": {
        "onx": "onx",
        "build": "onx Build",
