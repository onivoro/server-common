        "module": "commonjs"
    },
    "devDependencies": {
        "@onivoro/cli": "*",
        "@types/body-parser": "^1.19.5",
        "@types/jest": "*",
        "@types/node": "18.19.1",
        "typescript": "*"
    },
    "engines": {
        "node": "18.19.1",
        "npm": "10.2.4"
    },
    "dependencies": {
        "@nestjs/common": "^10.3.3",
        "@nestjs/core": "^10.3.3",
        "@nestjs/swagger": "^7.3.0",
        "@onivoro/server-common": "^0.1.1",
        "body-parser": "^1.20.2"
    }
